{
  "name": "my_yonsei_nft",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port=3000",
    "build": "tsc && vite build",
    "test": "jest --watchAll",
    "test:report": "jest --ci --reporters=default --reporters=jest-junit",
    "preview": "vite preview",
    "prepare": "husky install",
    "deploy": "aws s3 sync ./dist s3://devmyyonseinft --profile=myyonseinft",
    "invalidate": "aws cloudfront create-invalidation --profile=myyonseinft --distribution-id E2L17FEDPFWD7V --paths /*"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "axios": "^1.6.8",
    "embla-carousel-autoplay": "^8.0.2",
    "embla-carousel-react": "^8.0.2",
    "html2canvas": "^1.4.1",
    "qrcode.react": "^3.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^5.1.0",
    "react-loading": "^2.0.3",
    "react-redux": "^9.1.1",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.40",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.16",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.32",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
